{
  "manifest_version": 2,
  "name": "Ng Profiler",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content-script.bundle.js"]
    }
  ],
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "*"
  ]
}